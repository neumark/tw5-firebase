{
  "name": "authorized-https-endpoint-functions",
  "description": "restrict HTTPS functions to Firebase users",
  "dependencies": {
    "ajv": "^6.12.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "~8.9.2",
    "firebase-functions": "^3.3.0"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.1",
    "ava": "^3.8.2",
    "eslint": "^6.8.0",
    "eslint-plugin-promise": "^4.2.1",
    "firebase-functions-test": "^0.2.1",
    "sinon": "^9.0.2"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --max-warnings=0 .",
    "serve": "../node_modules/.bin/firebase serve --only functions",
    "shell": "../node_modules/.bin/firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "../node_modules/.bin/firebase deploy --only functions",
    "logs": "../node_modules/.bin/firebase functions:log",
    "test": "./node_modules/.bin/ava tests/*.spec.js",
    "debugtest": "./node_modules/.bin/ava --timeout 400000 --node-arguments=--inspect-brk tests/*.spec.js"
  },
  "engines": {
    "node": "10"
  },
  "private": true
}
